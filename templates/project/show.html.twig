{% extends 'base.html.twig' %}

{% block title %}Project{% endblock %}

{% block body %}
    <h1>Project</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ project.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ project.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ project.description }}</td>
            </tr>
            <tr>
                <th>Images</th>
                <td>{{ project.images }}</td>
            </tr>
            <tr>
                <th>Github</th>
                <td>{{ project.github }}</td>
            </tr>
            <tr>
                <th>Website</th>
                <td>{{ project.website }}</td>
            </tr>
            <tr>
                <th>Goal</th>
                <td>{{ project.goal }}</td>
            </tr>
            <tr>
                <th>Why</th>
                <td>{{ project.why }}</td>
            </tr>
            <tr>
                <th>More</th>
                <td>{{ project.more }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_project_index') }}">back to list</a>

    <a href="{{ path('app_project_edit', {'id': project.id}) }}">edit</a>

    {{ include('project/_delete_form.html.twig') }}
{% endblock %}
